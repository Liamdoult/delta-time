{
  "name": "tdelta",
  "version": "1.0.0",
  "description": "Date extention libirary for time difference.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "doc": "typedoc --out docs --mode modules src",
    "build": "rm -rf dist && tsc",
    "prestart": "rm -rf dist && tsc",
    "start": "node build/cli.js",
    "fmt": "prettier --write '{,*/**/}*.{js,jsx,ts,json,md}'",
    "fmt-check": "prettier --check '{,*/**/}*.{js,jsx,ts,json,md}'",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
  },
  "author": "Liam Doult",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "mocha": "^10.2.0",
    "nyc": "^15.0.1",
    "prettier": "2.0.4",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3",
    "@types/node": "^13.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm test -- --reporter min"
    }
  },
  "dependencies": {},
  "keywords": [
    "time",
    "delta",
    "interval",
    "time delta",
    "tdelta",
    "deltatime",
    "timedelta",
    "difference",
    "diff",
    "Date",
    "comparison",
    "milliseconds",
    "second",
    "minute",
    "hour",
    "day",
    "delta-time",
    "week",
    "parser",
    "timeout"
  ]
}
